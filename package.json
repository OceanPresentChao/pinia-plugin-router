{
  "name": "pinia-plugin-router",
  "version": "0.1.1",
  "description": "A pinia plugin that help use vue-router in pinia",
  "license": "MIT",
  "author": "OceanPresent <oceanpresent@163.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/OceanPresentChao/pinia-plugin-router.git"
  },
  "keywords": [
    "pinia",
    "plugin",
    "router",
    "typescript"
  ],
  "homepage": "https://github.com/OceanPresentChao/pinia-plugin-router",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "esno ./src/index.ts",
    "lint": "eslint .",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@types/node": "^17.0.23",
    "eslint": "^8.18.0",
    "esno": "^0.16.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.15.2",
    "vue": "^3.2.37",
    "pinia": "^2.0.14",
    "vue-router": "^4.1.1"
  },
  "dependencies": {}
}